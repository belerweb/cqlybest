<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cqlybest.common.bean">
	<class name="ProductDetailMaldives" table="PRODUCT_DETAIL_MALDIVES">
		<id name="id" column="ID" type="string" length="36">
			<generator class="assigned" />
		</id>
		<property name="room1" column="ROOM1" />
		<property name="room1Unit" column="ROOM1_UNIT" length="8" />
		<property name="room2" column="ROOM2" />
		<property name="room2Unit" column="ROOM2_UNIT" length="8" />
		<property name="room3" column="ROOM3" />
		<property name="room3Unit" column="ROOM3_UNIT" length="8" />
	</class>
</hibernate-mapping>