<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cqlybest.common.bean">
	<class name="ProductMaldives" table="PRODUCT_MALDIVES">
		<id name="productId" column="PRODUCT_ID" length="36">
			<generator class="assigned" />
		</id>
		<property name="maldivesIslandId" column="MALDIVES_ISLAND_ID" length="36" />
		<property name="maldivesRoomId" column="MALDIVES_ROOM_ID" />
	</class>
</hibernate-mapping>