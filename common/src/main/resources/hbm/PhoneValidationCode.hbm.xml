<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cqlybest.common.bean">
	<class name="PhoneValidationCode" table="PHONE_VALIDATION_CODE">
		<id name="phone" column="PHONE" type="string" length="11">
			<generator class="assigned" />
		</id>
		<property name="code" column="CODE" type="string" length="16" />
		<property name="createdTime" column="CREATED_TIME" />
	</class>
</hibernate-mapping>