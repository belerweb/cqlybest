<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cqlybest.common.bean">
	<class name="WeiboAppAuth" table="WEIBO_APP_AUTH">
		<id name="id" column="ID">
			<generator class="native" />
		</id>
		<property name="appId" column="APP_ID" length="36" />
		<property name="cid" column="CID" length="36" />
		<property name="uid" column="UID" length="36" />
		<property name="token" column="TOKEN" length="36" />
		<property name="expireIn" column="EXPIRE_IN" />
		<property name="createdTime" column="CREATED_TIME" />
		<property name="lastUpdate" column="LAST_UPDATE" />
	</class>
</hibernate-mapping>