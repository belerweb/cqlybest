<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cqlybest.common.bean">
	<class name="MaldivesSeaIsland" table="MALDIVES_SEA_ISLAND">
		<id name="id" column="ID" type="string" length="36">
			<generator class="assigned" />
		</id>
		<property name="zhName" column="ZH_NAME" length="64" />
		<property name="enName" column="EN_NAME" length="64" />
		<property name="level" column="LEVEL" length="32" />
		<property name="way" column="WAY" length="32" />
		<property name="area" column="AREA" length="32" />
		<property name="snorkeling" column="SNORKELING" length="32" />
		<property name="price" column="PRICE" length="32" />
		<property name="createdTime" column="CREATED_TIME" />
		<property name="lastUpdated" column="LAST_UPDATED" />
	</class>
</hibernate-mapping>