<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cqlybest.common.bean">
    <class name="AdvertorialHistrory" table="ADVERTORIAL_HISTRORY">
        <id name="id" column="ID">
            <generator class="native" />
        </id>
        <property name="aid" column="AID" />
        <property name="url" column="URL" type="string" not-null="true" length="256" />
        <property name="publishDate" column="PUBLISH_DATE" />
    </class>
</hibernate-mapping>